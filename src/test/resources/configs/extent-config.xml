<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme --> <!-- standard, dark -->
    <theme>dark</theme>
  
    <!-- document encoding -->  <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->   <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>Cucumber Framework</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName>Cucumber Extend Report</reportName>
    
    <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
     <reportName>
    <![CDATA[
            <img src='D:/Doorward/GUIAutomation/DoorwardRegression/DoorwardAutomation/src/main/java/utilities/d4.png' />
        ]]>              
    </reportName>
    
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
        $(".brand-logo").hide(); 
        });
      ]]>
    </scripts>
    
   <!-- custom styles -->
        <styles>
            <![CDATA[
                .report-name { padding-left: 10px; } .report-name > img { float: left;height: 92%;margin-left: 0px;margin-top: 2px;width: auto; }
            ]]>
        </styles>
  </configuration>
</extentreports>